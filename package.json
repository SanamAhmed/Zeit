  "devDependencies": {
    "@fitbit/sdk": "~3.1.3",
    "@fitbit/sdk-cli": "~1.7.0",
    "@types/jest": "~24.0.20",
    "@types/node": "~12.11.7",
    "fitbit-sdk-types": "~4.1.7",
    "jest": "~24.9.0",
    "ts-jest": "~24.1.0",
    "typescript": "~3.6.4"
